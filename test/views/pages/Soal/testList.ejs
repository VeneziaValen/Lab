<h1>Soal</h1>
<hr>

<a href="./soal" class="btn btn-success float-right">
    <i data-feather="plus" style="text-align: center; vertical-align: middle; align-items: center;"></i>Tambah Test</a>

<br><br>
<table class="table table-striped">
    <thead class="thead-dark">
        <tr>
            <th style="text-align: center;" scope="col">No.</th>
            <th style="text-align: center;" scope="col">Nama Test</th>
            <th style="text-align: center;" scope="col">Mata Kuliah</th>
            <th style="text-align: center;" scope="col">Semester</th>
            <th style="text-align: center;" scope="col">Waktu (menit)</th>
            <th style="text-align: center;" scope="col">Action</th>
        </tr>
    </thead>
    <tbody>
        <% if(data.length){
 
            for(var i = 0; i< data.length; i++) {%>  
            <tr>
              <th scope="row"><%= (i+1) %></th>
              <td style="text-align: center; vertical-align: middle;">
                <a href="../soalList/<%= data[i].idtests%>" style="color: blueviolet;"> <u><b><%= data[i].nama_test%></b></u> </a> </td>
              <td style="text-align: center; vertical-align: middle;"><%= data[i].nama_matkul%></td>
              <td style="text-align: center; vertical-align: middle;"><%= data[i].semester%></td>
              <td style="text-align: center; vertical-align: middle;"><%= data[i].waktu%></td>
              <td style="text-align: center; vertical-align: middle;">
              <a class="btn btn-success edit" href="./testEdit/<%= data[i].idtests%>">
                <i data-feather="edit"></i></a>                       
              <a class="btn btn-danger delete" onclick="return alert('Are You sure?')" href="/testList/delete/<%= data[i].idtests%>">
                <i data-feather="trash-2"></i></a>                       
             </td>
            </tr>
            <% }
                     
             }else{ %>
                 <tr>
                    <td colspan="6">No test</td>
                 </tr>
              <% } %>
    </tbody>
</table>

<%- contentFor('title') %> 
Daftar Test